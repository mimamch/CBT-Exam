              <div class="historyCard">
                  <p class="materi">Materi Soal : <%= e.MID.materiSoal %></p>
                  <p class="rightAnswer">Jawaban Benar : <%= e.jawabanBenar.length %></p>
                  <p class="nilai">Nilai : <%= e.nilai %></p>
                  <p class="startTime">Waktu Mulai : <%= e.TID.startTime %></p>
              </div>
         <% }) %> 
      </div>
    </div>
    <div class="column2">
        <div class="materi-wrapper">
            <h3>START NEW TEST</h3>
            <% materi.forEach((e, i) => {%>
            <a href="/client/new/<%= e._id %> " class="materi-card">
                <p class="materiSoal"><%= i +1 %>. <%= e.materiSoal %> </p>
            </a>
                <% }) %> 
        </div>
    </div>
